# Override default settings of GNOME Shell extension dash-to-dock
# (the extension doesn't use any schema)
[org/gnome/shell/extensions/dash-to-dock]
custom-theme-running-dots=true
dock-position='LEFT'
custom-theme-shrink=true
background-opacity=0.35
height-fraction=0.90
opaque-background=true
dash-max-icon-size=32
preferred-monitor=0
minimize-shift=true
